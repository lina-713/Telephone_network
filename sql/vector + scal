CREATE OR REPLACE FUNCTION public.fill_status(
	col_1 text,
	col_2 text)
    RETURNS TABLE(id integer, name text) 
    LANGUAGE 'plpgsql'
    COST 100
    VOLATILE PARALLEL UNSAFE
    ROWS 1000

AS $BODY$
begin
	
	RETURN QUERY EXECUTE format('select DISTINCT %I, %I from status', col_1, col_2);
end;
$BODY$;
